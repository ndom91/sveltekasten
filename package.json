{
  "name": "briefkasten-svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:push": "pnpm dlx prisma db push",
    "db:migrate": "pnpm dlx prisma migrate dev",
    "db:studio": "pnpm dlx prisma studio",
    "db:generate": "pnpm dlx prisma generate",
    "drizzle:migrate:new": "drizzle-kit generate:sqlite",
    "drizzle:push": "drizzle-kit push:sqlite",
    "drizzle:check": "drizzle-kit check:sqlite",
    "drizzle:studio": "drizzle-kit studio",
    "drizzle:start:local": "turso dev -f local.db",
    "postinstall": "pnpm dlx prisma generate"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "3.1.0",
