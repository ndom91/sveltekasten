{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "lib": ["dom", "esnext"],
    "sourceMap": true,
    "strict": true,
    "target": "ESNext",
    "module": "ESNext"
  },
  "ts-node": {
    "esm": true
  }
}
