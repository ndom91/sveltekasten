{
  "name": "sveltekasten-web",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --port 3000",
    "clean": "rm -rf build || true",
    "build": "pnpm clean && vite build && pnpm copy:partytown",
    "start": "node build/index.js",
    "preview": "vite preview",
    "format": "prettier --write src/**/*.{svelte,ts,json,css,html}",
    "test:e2e": "playwright test",
    "copy:partytown": "pnpm exec partytown copylib build/client/~partytown",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "pnpm check --watch",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "prepare": "svelte-kit sync"
  },
  "type": "module",
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@sveltejs/adapter-node": "^5.2.1",
    "@sveltejs/kit": "2.5.21",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.14",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.2.0",
    "@types/ws": "^8.5.12",
    "autoprefixer": "^10.4.20",
    "open-props": "^1.7.5",
    "postcss": "^8.4.41",
    "postcss-nesting": "^13.0.0",
    "prettier-plugin-svelte": "3.2.6",
    "prisma": "5.18.0",
    "svelte": "5.0.0-next.212",
    "svelte-check": "3.8.5",
    "svelte-headless-table": "^0.18.2",
    "tailwindcss": "^3.4.9",
    "typescript-svelte-plugin": "^0.3.40",
    "workbox-expiration": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.2",
    "@auth/sveltekit": "^1.4.2",
    "@builder.io/partytown": "^0.10.2",
    "@formkit/tempo": "^0.1.2",
    "@internationalized/date": "^3.5.5",
    "@prisma/client": "5.18.0",
    "@xenova/transformers": "^2.17.2",
    "bits-ui": "^0.21.13",
    "clsx": "^2.1.1",
    "cmdk-sv": "^0.0.18",
    "formsnap": "^1.0.1",
    "isomorphic-dompurify": "^2.14.0",
    "metascraper": "^5.45.22",
    "metascraper-author": "^5.45.22",
    "metascraper-date": "^5.45.22",
    "metascraper-description": "^5.45.22",
    "metascraper-feed": "^5.45.22",
    "metascraper-image": "^5.45.22",
    "metascraper-lang": "^5.45.22",
    "metascraper-publisher": "^5.45.22",
    "metascraper-readability": "^5.45.22",
    "metascraper-title": "^5.45.22",
    "metascraper-url": "^5.45.22",
    "mode-watcher": "^0.4.1",
    "rm": "^0.1.8",
    "runed": "^0.15.0",
    "sharp": "^0.33.4",
    "svelte-infinite": "^0.3.2",
    "svelte-sonner": "^0.3.27",
    "sveltekit-superforms": "^2.16.1",
    "tailwind-merge": "^2.4.0",
    "tailwind-variants": "^0.2.1",
    "thumbhash": "^0.1.1",
    "vaul-svelte": "^0.3.2",
    "ws": "^8.18.0",
    "zod": "^3.23.8",
    "zod-prisma-types": "^3.1.8"
  }
}
