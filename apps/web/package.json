{
  "name": "sveltekasten-web",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev --port 3000",
    "clean": "rm -rf build || true",
    "build": "pnpm clean && vite build && pnpm copy:partytown",
    "start": "node build/index.js",
    "preview": "vite preview",
    "format": "prettier --write src/**/*.{svelte,ts,json,css,html}",
    "test:e2e": "playwright test",
    "copy:partytown": "pnpm exec partytown copylib build/client/~partytown",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "pnpm check --watch",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "prepare": "svelte-kit sync"
  },
  "type": "module",
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@sveltejs/adapter-node": "^5.3.3",
    "@sveltejs/kit": "2.46.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.7.2",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.21",
    "open-props": "^1.7.16",
    "postcss": "^8.5.6",
    "postcss-nesting": "^13.0.2",
    "prettier-plugin-svelte": "3.4.0",
    "prisma": "6.17.1",
    "svelte": "5.39.12",
    "svelte-check": "4.3.3",
    "svelte-headless-table": "^0.18.3",
    "tailwindcss": "^4.1.14",
    "typescript-svelte-plugin": "^0.3.50",
    "workbox-expiration": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@auth/sveltekit": "^1.11.0",
    "@builder.io/partytown": "^0.10.2",
    "@formkit/tempo": "^0.1.2",
    "@internationalized/date": "^3.10.0",
    "@prisma/adapter-pg": "^6.17.1",
    "@prisma/client": "6.17.1",
    "@xenova/transformers": "^2.17.2",
    "bits-ui": "^2.11.5",
    "clsx": "^2.1.1",
    "cmdk-sv": "^0.0.19",
    "formsnap": "^2.0.1",
    "isomorphic-dompurify": "^2.29.0",
    "metascraper": "^5.49.4",
    "metascraper-author": "^5.49.2",
    "metascraper-date": "^5.49.2",
    "metascraper-description": "^5.49.2",
    "metascraper-feed": "^5.49.2",
    "metascraper-image": "^5.49.2",
    "metascraper-lang": "^5.49.2",
    "metascraper-readability": "^5.49.4",
    "metascraper-title": "^5.49.2",
    "metascraper-url": "^5.49.2",
    "metascraper-x": "^5.49.2",
    "mode-watcher": "^1.1.0",
    "rm": "^0.1.8",
    "runed": "^0.35.0",
    "sharp": "^0.34.4",
    "svelte-infinite": "^0.5.0",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "^2.27.3",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "thumbhash": "^0.1.1",
    "vaul-svelte": "^0.3.2",
    "ws": "^8.18.3",
    "zod": "^4.1.12",
    "zod-prisma-types": "^3.3.5"
  }
}
